<div class="row">
    <div class="col-md-offset-2 col-md-8">
        <div class="panel panel-default">
        <div class="panel-heading center"><%= image_tag @pin.image.url(:medium)%></div>
        <div class="panel-body">
            <center><%= @pin.description %><br/></center>
            <% if @pin.user == current_user%>
            <%= link_to 'Editar | ', edit_pin_path(@pin) %>
            <%end%>
           <%= link_to 'Voltar', pins_path %>
        </div>
    </div>
 </div>
</div>





